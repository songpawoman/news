<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<typeAlias type="org.sp.news.domain.News" 				alias="News"/>
		<typeAlias type="org.sp.news.domain.Comments" 		alias="Comments"/>
		<typeAlias type="org.sp.news.domain.Member" 		alias="Member"/>
		<typeAlias type="org.sp.news.domain.Hobby" 			alias="Hobby"/>
		<typeAlias type="org.sp.news.domain.UserHobby" 	alias="UserHobby"/>
		<typeAlias type="org.sp.news.domain.Gallery" 			alias="Gallery"/>
		<typeAlias type="org.sp.news.domain.GalleryImg" 	alias="GalleryImg"/>
	</typeAliases>
		
	<mappers>
		<mapper resource="org/sp/news/mybatis/NewsMapper.xml" />
		<mapper resource="org/sp/news/mybatis/CommentsMapper.xml" />
		<mapper resource="org/sp/news/mybatis/MemberMapper.xml" />
		<mapper resource="org/sp/news/mybatis/HobbyMapper.xml" />
		<mapper resource="org/sp/news/mybatis/UserHobbyMapper.xml" />
		<mapper resource="org/sp/news/mybatis/GalleryMapper.xml" />
		<mapper resource="org/sp/news/mybatis/GalleryImgMapper.xml" />
		
	</mappers>
</configuration>